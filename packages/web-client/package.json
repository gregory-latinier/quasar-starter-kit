    "babel-eslint": "10.0.1",
    "dotenv": "7.0.0",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-vue": "5.2.2",
    "jsonwebtoken": "8.5.1",
